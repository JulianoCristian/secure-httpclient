<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>SecureHttpClient</id>
    <version>$version$</version>
    <title>Secure HttpClient</title>
    <authors>tranb3r</authors>
    <owners>tranb3r</owners>
	<description>Cross-platform HttpClientHandler with TLS1.2 and Certificate Pinning</description>
    <tags></tags>
	<projectUrl>https://github.com/zitch-code/secure-httpclient</projectUrl>
    <licenseUrl>https://raw.github.com/zitch-code/secure-httpclient/master/LICENSE</licenseUrl>	
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <group targetFramework="MonoAndroid81">
        <dependency id="Square.OkHttp3" version="3.8.1" />
        <dependency id="Square.OkIO" version="1.13.0" />
        <dependency id="Microsoft.Extensions.Logging.Abstractions" version="2.0.2" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="Portable.BouncyCastle" version="1.8.1.4" />
        <dependency id="Microsoft.Extensions.Logging.Abstractions" version="2.0.2" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="Portable.BouncyCastle" version="1.8.1.4" />
        <dependency id="Microsoft.Extensions.Logging.Abstractions" version="2.0.2" />
      </group>
    </dependencies>
  </metadata>

  <files>
    <file src="bin\monoandroid81\SecureHttpClient.dll" target="lib\monoandroid81" />
    <file src="bin\xamarin.ios10\SecureHttpClient.dll" target="lib\xamarin.ios10" />
	<file src="bin\netstandard2.0\SecureHttpClient.dll" target="lib\netstandard2.0" />
  </files>
</package>
